<ul>
<% @result.each do |space_object|%>
  <li> <a href='/spaces/<%= space_object.id %>' target="_blank"><%=space_object.name%></a><%=space_object.user_name%><%=space_object.price%>
<%end%>
</li>
</ul>
 <% if @user %>
  <h1> Welcome, <%= @user.display_name %></h1>

<form action='/spaces/new' method='get'>
  <input type='submit' value='Add Space'>
</form>

<% end %>
<% unless @user %>
<form action="/users/new" method="get">
  <input type="submit" value="Sign up" />
</form>
<% end %>
